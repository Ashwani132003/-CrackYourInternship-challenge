class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        
        i=0
        j=0
        while i < len(nums)-1:
            if nums[i] != nums[i+1]:
                nums[j] = nums[i]
                j+=1
         
            i+=1
        if(i>j):    
            t=nums[-1]
            nums.pop()
            if(nums[j-1]!=t):
                nums[j]=t
        return j+1   
